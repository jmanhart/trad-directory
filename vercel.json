{
  "buildCommand": "npm run build",
  "installCommand": "npm install --include=dev",
  "framework": "vite",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "env": {
    "NPM_CONFIG_PRODUCTION": "false"
  },
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  }
}