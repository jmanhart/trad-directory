{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "vite",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "src/**/*.ts": {
      "maxDuration": 10
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  }
}
